{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "DetailedErrors": true,
  "HostingEnvironment": "Development",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "AISAM.API",
    "Audience": "AISAM.Client",
    "ExpirationHours": 24
  },
  "FacebookSettings": {
    "AppId": "",
    "AppSecret": "",
    "RedirectUri": "http://localhost:5000/auth/facebook/callback",
    "GraphApiVersion": "v20.0",
    "BaseUrl": "https://graph.facebook.com",
    "OAuthUrl": "https://www.facebook.com",
    "RequiredPermissions": [
      "pages_manage_posts",
      "pages_read_engagement",
      "pages_show_list"
    ]
  }
}
